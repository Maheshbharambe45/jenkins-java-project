pipeline {
 agent any
stages{
   stage("checkout"){
     steps {
          sh """
           echo "Checkout done"
        ls -lrt
       """
      }
   }

 stage("building the application"){
     steps {
          sh """
  echo "building the application"
         mvn clean package  
  echo "builded  the application"
          """
      }
   }



} // end of stages

} // end of pipeline
